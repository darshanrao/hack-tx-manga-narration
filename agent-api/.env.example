############################################################
# Backend environment variables (example placeholders only)
# Do NOT put real secrets in this file.
# Copy to .env and fill in your own values locally.
############################################################

# Supabase (optional for local dev without DB)
# Example: https://YOUR-PROJECT-REF.supabase.co
SUPABASE_URL=YOUR_SUPABASE_URL
# Supabase anon/public key (treat as sensitive in server contexts)
SUPABASE_KEY=YOUR_SUPABASE_ANON_KEY

# ElevenLabs API key (optional unless using TTS)
ELEVENLABS_API_KEY=YOUR_ELEVENLABS_API_KEY

# Google AI Studio (Gemini)
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY
# Example: gemini-2.5-flash, gemini-2.5-pro
GEMINI_MODEL=
